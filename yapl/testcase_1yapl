	{
		poornank a;
		bada dashamlav c;
	};

	poornank *jj = &auto;

	System.out.print("java here");
	akshar echo[3] = "bash here";
	myprintf("CS3300 here");
	printf(echo);

	/*
		mixing things here a bit
	*/

	agar (a == 9)
	{
		//NO-OP
	}
	
	agar (a == 1)
		hh(++jj);
	warna agar (a == 2)
		hh(jj++);
	warna agar (a == 3)
		hh(*jj++);
	warna
	{
		//NO-OP
	}
			
