poornank *hh(akshar *p)
{
	poornank n = 7;

	scanf("%d", &n);
	
	agar (n == 0)
		printf("%d\n", n + 1);
	warna agar (n == 1)
		printf("%d\n", n + 2);
	
	waapas kar (NULL);
}
